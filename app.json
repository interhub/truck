{
    "expo": {
        "android": {
            "package": "com.interhub.truck",
            "versionCode": 2
        },
        "name": "Грузоперевозки в Калининграде",
        "slug": "truck",
        "version": "2.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./src/img/load.png",
            "resizeMode": "cover",
            "backgroundColor": "#00ff0c"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "packagerOpts": {
            "sourceExts": [
                "ts",
                "tsx"
            ],
            "transformer": "node_modules/react-native-typescript-transformer/index.js"
        }
    }
}
